{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/display-meta",
  "version": "0.1.0",
  "title": "Display Meta",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "metaType": {
      "type": "string",
      "default": "native"
    },
    "metaKey": {
      "type": "string"
    }
  },
  "usesContext": [
    "queryId",
    "postId",
    "postType"
  ],
  "textdomain": "display-meta",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}